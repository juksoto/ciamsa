[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}
.icon-rocket:before {
	content: "\e600";
}
.icon-star:before {
	content: "\e601";
}
.icon-plus:before {
	content: "\e602";
}
.icon-mail:before {
	content: "\e603";
}
